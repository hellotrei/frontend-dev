{
  "extends": ["airbnb"],
  "plugins": [
    "react",
    "import",
    "jsx-a11y",
    "react-hooks"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".tsx"] }],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/prop-types": 0
  }
}
