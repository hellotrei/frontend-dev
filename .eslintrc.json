{
  "settings": {
      "react": {
          "version": "detect"
      }
  },
  "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "commonjs": true,
      "es6": true
  },
  "extends": [
    "eslint:recommended",
    "next",
    "plugin:@next/next/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
  }
}